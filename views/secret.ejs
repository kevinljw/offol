<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>loffo buying page</title>

    <!-- Bootstrap Core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="css/loffo_web.css" rel="stylesheet">
    <script src="js/jquery.js"></script>
    <script src="js/lottoWeb_buy.js"></script>
</head>

<body>

    

    <!-- Page Content -->
    <div class="container">
        <h2> Loffo Buy Page </h2>
        <hr class="featurette-divider">

        

        <!-- First Featurette -->
        <form name="myForm">
             <div class="checkbox">
            <label>
              <input type="checkbox" id="sendEmailChk" name="seChk"> Send Email?
            </label>
          </div>
          <div class="form-group">
            <label for="exampleInputEmail">Student ID or Email address</label>
            <input type="text" class="form-control" id="exampleInputEmail" placeholder="d00xxxxxx or r00xxxxxx" name="Email">
          </div>
          <div class="form-group">
            <label for="exampleInputNum">Amount</label>
            <input type="text" class="form-control" id="exampleInputNum" placeholder="Amount" name="Num">
          </div>

          <div class="checkbox">
            <label>
              <input type="checkbox" id="chkBox" name="Chk"> Check me out
            </label>
          </div>
          <button type="submit" class="btn btn-primary" id="submitBuyBtn">Submit</button>
</form>
        

        <hr class="featurette-divider">


        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p><br>
                    Copyright &copy; kevinljw 2015
                    </p>

                </div>
            </div>
        </footer>

    </div>
    <!-- /.container -->

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

</body>

